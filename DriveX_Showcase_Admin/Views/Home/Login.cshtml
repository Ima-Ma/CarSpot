
@model Dealer_Account
<div class="container-scroller">
    <div class="container-fluid page-body-wrapper full-page-wrapper">
        <div class="content-wrapper d-flex align-items-center auth px-0">
            <div class="row w-100 mx-0">
                <div class="col-lg-4">
                    <img src="~/login.jpg" class="img-fluid" />
                </div>
                <div class="col-lg-8 " style="margin-top:100px;">
                    
                        <h4> Login as Dealer</h4>
                        <form class="pt-3" method="post">
                            <div class="form-group">
                                <input type="email" name="email" class="form-control form-control-lg"  placeholder="Email">
                            </div>
                            <div class="form-group">
                                <input type="text" name="dealer_IdCode" class="form-control form-control-lg" placeholder="Dealer Id Code">
                            </div>
                           
                            <div class="text-center mt-4 font-weight-light">
                          <button type="submit" asp-controller="Home" asp-action="Login" class="btn btn-primary w-100 ">Login</button>
                            </div>
                        </form>
                 
                </div>
            </div>
        </div>
    </div>
</div>


