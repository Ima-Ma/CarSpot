@Html.Partial("Layout")
@Html.Partial("_Header")
@{
    Layout = null;
}
<main class="main min-vh-100" id="top">

    <!-- ============================================--><!-- <section> begin ============================-->
    <section class="py-0">
        <div class="container-fluid">
            <div class="row">
                <div class="col-lg-6 px-0 order-lg-2">
                    <div class="sticky-top vh-lg-100 py-9">
                        <div class="bg-holder" style="background-image:url('https://img.freepik.com/free-vector/oops-404-error-with-broken-robot-concept-illustration_114360-5529.jpg');" data-zanim-trigger="scroll" data-zanim-lg='{"animation":"zoom-out-slide-left","delay":0.4}'></div><!--/.bg-holder-->
                    </div>
                </div>
                <div class="col-lg-6 py-6">
                    <div class="row h-100 flex-center">
                        <div class="col-lg-8">
                            <h1 class="display-4 fs-7 fs-lg-6 fs-xl-5"><span class="d-block overflow-hidden pb-1"><span class="d-inline-block" data-zanim-xs='{"delay":0.4}' data-zanim-trigger="scroll">Trying to get</span></span><span class="d-block overflow-hidden pb-1"><span class="d-inline-block" data-zanim-xs='{"delay":0.4}' data-zanim-trigger="scroll">where you want</span></span><span class="d-block overflow-hidden pb-1"><span class="d-inline-block" data-zanim-xs='{"delay":0.4}' data-zanim-trigger="scroll">to go?</span></span></h1>
                            <div class="overflow-hidden">
                                <p class="fs-8 mt-4" data-zanim-xs='{"delay":0.5}' data-zanim-trigger="scroll">This page isn't it.</p>
                            </div>
                            <div class="overflow-hidden">
                                <div data-zanim-xs='{"delay":0.6}' data-zanim-trigger="scroll"><a class="btn btn-outline-dark mt-6 mb-1" asp-controller="User" asp-action="Index" >take me User</a></div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div><!-- end of .container-->
    </section><!-- <section> close ============================--><!-- ============================================-->

</main><!-- ===============================================--><!--    End of Main Content--><!-- ===============================================-->
@Html.Partial("_Footer")
@Html.Partial("Layoutscript")